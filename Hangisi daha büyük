#include <iostream>
#include <ctime>
#include <stdio.h>
#include<locale.h>
#pragma warning (disable:4996)
using namespace std;

int islem(int x, int y) {
	
	if (x < y) {
		cout << y << " daha buyuktur.";
}else {
	cout << x << " daha buyuktur.";
		}
	return 0;
}

int main() {
	setlocale(LC_ALL, "Turkish");
	int x, y;
	cout << "1.say覺y覺 giriniz: ";
	cin >> x;
	cout << "2.say覺y覺 giriniz: ";
	cin >> y;
	islem(x, y);
	return 0;
}
